{
    "name": "yoeunes/notify-symfony",
    "type": "symfony-bundle",
    "description": "Symfony Notify Bundle for yoeunes/notify",
    "keywords": [
        "yoeunes",
        "notify",
        "php",
        "laravel",
        "symfony",
        "Lumen",
        "notifications",
        "messages",
        "alerts",
        "pnotify",
        "toastr ",
        "bundle",
        "flex"
    ],
    "homepage": "https://github.com/yoeunes/notify-symfony",
    "authors": [
        {
            "name": "Younes Khoubza",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://github.com/yoeunes",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "yoeunes/notify": "v2.x-dev",
        "symfony/config": "^2.7|^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^2.7|^3.0|^4.0|^5.0",
        "symfony/http-kernel": "^2.7|^3.0|^4.0|^5.0",
        "twig/twig": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Yoeunes\\Notify\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoeunes\\Notify\\Symfony\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.3.9"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
